  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug functions",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/functions.js",
      "runtimeExecutable": "/Users/shripada/.nvm/versions/node/v18.8.0/bin/node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug prime module",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/prime.asserts.js"
    },
    {
      "name": "Debug Jest Tests",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/.bin/jest",
        "--runInBand"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug fs",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/fs.js",
      "args": ["."],
      "runtimeExecutable": "/Users/shripada/.nvm/versions/node/v18.8.0/bin/node"
    }
  ]
}
